<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--加载框-->
    <declare-styleable name="LoadingFrameView">
        <attr name="fv_emptyIcon" format="reference" />
        <attr name="fv_emptyInfo" format="string" />
        <attr name="fv_noIcon" format="reference" />
        <attr name="fv_noInfo" format="string" />
        <attr name="fv_repeatIcon" format="reference" />
        <attr name="fv_repeatInfo" format="string" />
        <attr name="fv_repeatBt" format="string" />
    </declare-styleable>

    <!--LoadingDialog-->
    <attr name="speed">
        <enum name="speedOne" value="1" />
        <enum name="speedTwo" value="2" />
    </attr>
    <attr name="strokeColor" format="color" />
    <declare-styleable name="RightDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>
    <declare-styleable name="WrongDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>


    <!--着色器-->
    <declare-styleable name="ColorWheel">
        <!-- 背景圆的半径 -->
        <attr name="circle_radius" format="dimension" />
        <!-- 可滑动小球的半径 -->
        <attr name="center_radius" format="dimension" />
        <!-- 可滑动小球的颜色 -->
        <attr name="center_color" format="color" />
    </declare-styleable>

    <!--填充验证码-->
    <declare-styleable name="PhoneCodeView">
        <!-- 默认颜色 -->
        <attr name="color_default" format="color" />
        <!-- 选中颜色 -->
        <attr name="color_focus" format="color" />
        <!-- 第四个文字颜色 -->
        <attr name="code4_color" format="color" />
        <!-- 第四个光标颜色 -->
        <attr name="vline4_bg" format="color" />
    </declare-styleable>


</resources>